{
  "name": "hanged-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "npm run start:both",
    "start:both": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "test:unit-with-backend": "concurrently \"npm run start:backend\" \"npm run test:unit\"",
    "start:frontend": "vite",
    "start:backend": ".venv\\Scripts\\activate && cd backend && flask run",
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/ --coverage"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@vue/test-utils": "^2.4.1",
    "axios": "^1.4.0",
    "jsdom": "^22.1.0",
    "process": "^0.11.10",
    "start-server-and-test": "^2.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.6",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.3",
    "concurrently": "^8.2.1",
    "electron": "^24.4.0",
    "electron-builder": "^24.6.3",
    "sass": "^1.64.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^0.34.3",
    "vue-tsc": "^1.4.2"
  },
  "main": "dist-electron/main.js"
}
